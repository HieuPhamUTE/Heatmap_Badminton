    "import warnings\n",
    "warnings.simplefilter(action='ignore', category=FutureWarning)\n",
    "\n",
    "import torch\n",
    "import cv2\n",
    "import os\n",
    "from tqdm import tqdm\n",
    "\n",
    "# === Load YOLOv5 model ===\n",
    "model = torch.hub.load('ultralytics/yolov5', 'yolov5s', pretrained=True)\n",
    "model.classes = [0]  # Chá»‰ nháº­n diá»‡n ngÆ°á»i (class 0 trong COCO dataset)\n",
    "\n",
    "# === ÄÆ°á»ng dáº«n video vÃ  thÆ° má»¥c lÆ°u video Ä‘áº§u ra ===\n",
    "video_path = '/kaggle/input/baminton/badminton2.mp4'  # ÄÆ°á»ng dáº«n video gá»‘c\n",
    "output_video_path = 'output_video.mp4'  # ÄÆ°á»ng dáº«n video sau khi xá»­ lÃ½\n",
    "\n",
    "# === Äá»c video ===\n",
    "cap = cv2.VideoCapture(video_path)\n",
    "frame_width = int(cap.get(cv2.CAP_PROP_FRAME_WIDTH))\n",
    "frame_height = int(cap.get(cv2.CAP_PROP_FRAME_HEIGHT))\n",
    "frame_count = int(cap.get(cv2.CAP_PROP_FRAME_COUNT))\n",
    "\n",
    "# === Thiáº¿t láº­p Ä‘á»ƒ lÆ°u video Ä‘áº§u ra ===\n",
    "fourcc = cv2.VideoWriter_fourcc(*'mp4v')  # Äá»‹nh dáº¡ng codec cho video .mp4\n",
    "out = cv2.VideoWriter(output_video_path, fourcc, 30.0, (frame_width, frame_height))\n",
    "\n",
    "# === Láº·p qua tá»«ng frame vÃ  nháº­n diá»‡n ngÆ°á»i ===\n",
    "for _ in tqdm(range(frame_count), desc=\"Processing frames\"):\n",
    "    ret, frame = cap.read()\n",
    "    if not ret:\n",
    "        break\n",
    "\n",
    "    # Step 1: DÃ¹ng YOLOv5 nháº­n diá»‡n ngÆ°á»i\n",
    "    results = model(frame)\n",
    "    boxes = results.xyxy[0].cpu().numpy()  # Láº¥y tá»a Ä‘á»™ bounding boxes cá»§a ngÆ°á»i\n",
    "\n",
    "    # Step 2: Lá»c vÃ  chá»‰ láº¥y 2 bounding box lá»›n nháº¥t\n",
    "    if len(boxes) > 0:\n",
    "        # Sáº¯p xáº¿p bounding boxes theo diá»‡n tÃ­ch (diá»‡n tÃ­ch = (x2 - x1) * (y2 - y1))\n",
    "        boxes = sorted(boxes, key=lambda b: (b[2] - b[0]) * (b[3] - b[1]), reverse=True)[:2]\n",
    "        \n",
    "        # Step 3: Váº½ bounding box quanh 2 ngÆ°á»i chÆ¡i chÃ­nh\n",
    "        for box in boxes:\n",
    "            x1, y1, x2, y2, conf, cls = box\n",
    "            if conf > 0.5:  # Chá»‰ váº½ náº¿u Ä‘á»™ tin cáº­y > 50%\n",
    "                cv2.rectangle(frame, (int(x1), int(y1)), (int(x2), int(y2)), (255, 0, 0), 2)\n",
    "    \n",
    "    # Step 4: LÆ°u frame Ä‘Ã£ váº½ bounding box vÃ o video Ä‘áº§u ra\n",
    "    out.write(frame)\n",
    "\n",
    "cap.release()\n",
    "out.release()\n",
    "print(f\"Video Ä‘Ã£ Ä‘Æ°á»£c lÆ°u táº¡i: {output_video_path}\")\n"
   ]
  },
