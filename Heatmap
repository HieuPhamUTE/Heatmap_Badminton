   "source": [
    "import cv2\n",
    "import mediapipe as mp\n",
    "import pandas as pd\n",
    "\n",
    "# MediaPipe khá»Ÿi táº¡o\n",
    "mp_pose = mp.solutions.pose\n",
    "pose = mp_pose.Pose(static_image_mode=False, min_detection_confidence=0.5, min_tracking_confidence=0.5)\n",
    "\n",
    "# ÄÆ°á»ng dáº«n file\n",
    "input_path = '/kaggle/input/baminton/output_video.mp4'  # Thay báº±ng thÆ° má»¥c báº¡n Ä‘Ã£ upload\n",
    "output_video_path = '/kaggle/working/foot_landmarks_video.mp4'\n",
    "output_csv_path = '/kaggle/working/foot_positions.csv'\n",
    "\n",
    "# Má»Ÿ video\n",
    "cap = cv2.VideoCapture(input_path)\n",
    "width = int(cap.get(cv2.CAP_PROP_FRAME_WIDTH))\n",
    "height = int(cap.get(cv2.CAP_PROP_FRAME_HEIGHT))\n",
    "fps = cap.get(cv2.CAP_PROP_FPS)\n",
    "\n",
    "# Ghi video Ä‘áº§u ra\n",
    "fourcc = cv2.VideoWriter_fourcc(*'mp4v')\n",
    "out = cv2.VideoWriter(output_video_path, fourcc, fps, (width, height))\n",
    "\n",
    "# Data cho CSV\n",
    "all_data = []\n",
    "frame_idx = 0\n",
    "\n",
    "while cap.isOpened():\n",
    "    ret, frame = cap.read()\n",
    "    if not ret:\n",
    "        break\n",
    "\n",
    "    img_rgb = cv2.cvtColor(frame, cv2.COLOR_BGR2RGB)\n",
    "    results = pose.process(img_rgb)\n",
    "\n",
    "    landmarks_list = []\n",
    "\n",
    "    if results.pose_landmarks:\n",
    "        # Trong video cÃ³ sáºµn 2 ngÆ°á»i, giáº£ Ä‘á»‹nh MediaPipe detect Ä‘Æ°á»£c cáº£ 2.\n",
    "        # Náº¿u khÃ´ng, cáº§n dÃ¹ng YOLO Ä‘á»ƒ detect bbox vÃ  cháº¡y pose tá»«ng ngÆ°á»i riÃªng.\n",
    "\n",
    "        # Giáº£ Ä‘á»‹nh MediaPipe chá»‰ tráº£ vá» 1 ngÆ°á»i => báº¡n cáº§n tá»± thÃªm bÆ°á»›c detect 2 ngÆ°á»i náº¿u cáº§n thiáº¿t\n",
    "        # á» Ä‘Ã¢y táº¡m coi báº¡n Ä‘Ã£ cÃ³ logic Ä‘á»ƒ cÃ³ danh sÃ¡ch 2 ngÆ°á»i trong landmarks_list\n",
    "        # Táº¡m thá»i chá»‰ xá»­ lÃ½ 1 ngÆ°á»i: báº¡n cÃ³ thá»ƒ má»Ÿ rá»™ng sau\n",
    "        landmarks = results.pose_landmarks.landmark\n",
    "\n",
    "        # Láº¥y gÃ³t chÃ¢n trÃ¡i/pháº£i\n",
    "        left_heel = landmarks[mp_pose.PoseLandmark.LEFT_HEEL]\n",
    "        right_heel = landmarks[mp_pose.PoseLandmark.RIGHT_HEEL]\n",
    "\n",
    "        l_heel = (int(left_heel.x * width), int(left_heel.y * height))\n",
    "        r_heel = (int(right_heel.x * width), int(right_heel.y * height))\n",
    "\n",
    "        landmarks_list.append({'left_heel': l_heel, 'right_heel': r_heel})\n",
    "\n",
    "    # GÃ¡n player1, player2 theo vá»‹ trÃ­ trÃ¡i/pháº£i\n",
    "    def get_avg_x(lm):  # trung bÃ¬nh x Ä‘á»ƒ biáº¿t bÃªn trÃ¡i hay pháº£i\n",
    "        return (lm['left_heel'][0] + lm['right_heel'][0]) / 2\n",
    "\n",
    "    sorted_players = sorted(landmarks_list, key=get_avg_x)\n",
    "\n",
    "    for i, player in enumerate(sorted_players):\n",
    "        player_id = f'player{i+1}'\n",
    "\n",
    "        lx, ly = player['left_heel']\n",
    "        rx, ry = player['right_heel']\n",
    "\n",
    "        # Váº½ lÃªn khung hÃ¬nh\n",
    "        cv2.circle(frame, (lx, ly), 8, (255, 0, 0), -1)\n",
    "        cv2.circle(frame, (rx, ry), 8, (0, 255, 0), -1)\n",
    "        cv2.putText(frame, player_id, (int((lx+rx)/2), int((ly+ry)/2)-10), cv2.FONT_HERSHEY_SIMPLEX, 0.6, (255,255,255), 2)\n",
    "\n",
    "        all_data.append({\n",
    "            'frame': frame_idx,\n",
    "            'player': player_id,\n",
    "            'left_heel_x': lx,\n",
    "            'left_heel_y': ly,\n",
    "            'right_heel_x': rx,\n",
    "            'right_heel_y': ry\n",
    "        })\n",
    "\n",
    "    out.write(frame)\n",
    "    frame_idx += 1\n",
    "\n",
    "# LÆ°u CSV\n",
    "df = pd.DataFrame(all_data)\n",
    "df.to_csv(output_csv_path, index=False)\n",
    "\n",
    "# ÄÃ³ng video\n",
    "cap.release()\n",
    "out.release()\n",
    "pose.close()\n"
   ]
  },
